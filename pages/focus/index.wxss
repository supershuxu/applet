view .active_tab_11 {
  font-size: 34rpx;
  font-weight: 800;
}

view .tabstyle {
  width: 15%;
  flex: none;
  -webkit-flex: none;
  text-align: left;
}

.content {
  padding-top: 30rpx;
}

.interested {
  width: 264rpx;
  display: inline-block;
  margin-top: 15rpx;
  font-size: 28rpx;
  color: #333333;
  letter-spacing: 1.2rpx;
  line-height: 40rpx;
  font-weight: 700;
}

.change {
  width: 426rpx;
  text-align: right;
  display: inline-block;
  font-size: 24rpx;
  color: #999999;
  letter-spacing: 1.5rpx;
}

.cardweap {
  display: flex;
  margin-top: 40rpx;
  overflow-x: auto;
  margin-bottom: 30rpx;
}

.card {
  flex-shrink:0;
  position: relative;
  text-align: center;
  width: 260rpx;
  height: 260rpx;
  background: #FFFFFF;
  box-sizing: border-box;
  padding-top: 30rpx;
  margin-right: 20rpx;
  border: solid 1rpx #f5ebeb;
  border-radius: 10rpx;
}
view .van-image{
  margin: 0;
}
.card_name {
  margin: 20rpx 0;
  font-size: 32rpx;
  color: #333333;
  letter-spacing: 1.4rpx;
}
.card_pic{
  width: 20rpx;
  height: 20rpx;
  position: absolute;
  top: 20rpx;
  right: 20rpx;
}

.midcontent{


}
.userinfo{
  width:100% ;
  display: inline-block;
  position: relative;
  padding-top: 30rpx;
}
.name{
  position: absolute;
  top: 30rpx;
  font-weight: 700;
  color: #333333 ;
  padding-left:40rpx ;
  left: 60rpx;
  font-size: 28rpx;
color: #333333;
letter-spacing: 0.35px;
}
.desc{
  position: absolute;
  top: 70rpx;
  color: #666666;
  padding-left:40rpx ;
  left: 60rpx;
  font-size: 12px;
color: #999999;
letter-spacing: 0.3px;
}
.myzhuye{
  position: absolute;
  color: #666666;
  right: 0;
  top: 70rpx;
  margin-top:-35rpx ;
  height: 34rpx;
  font-weight: 800;
}
.article{
  box-sizing: border-box;
  padding-top: 10rpx;
  border-bottom: 2px solid #f7f7f7
}
.article_title{
  width: 100%;
  font-weight: 900;
  font-size: 28rpx;
  line-height: 40rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-break: break-all;
}
.article_content{
  letter-spacing: 0.7px;
  width: 100%;
  line-height: 34rpx;
  margin-top: 20rpx;
  color: #333333;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
.article_info{
  position: relative;
  height: 74rpx;
}
.po{
  position: absolute;
  display: inline-block;
  top: 50%;
  margin-top: -17rpx;
  color: #999999 ;
  font-size: 24rpx;
  line-height: 37rpx;
}
.van-image{
position: absolute;
top: 30%;
margin-right: 20rpx;
}
.article_info_mark{
  color: #999999;
  line-height: 30rpx;
  text-align: center;
  right: 140rpx;
  margin-top: -15rpx;
}
.article_info_pinglun{
  right: 0;
}
view .van-icon{
  top: 4rpx;
  margin-right: 4px;
}
.po1{
  left: 50rpx;
}
.po2{
  background: #FFB1B1;
  color: white;
  width: 70rpx;
  height: 30rpx;
  line-height: 30rpx;
}
.van-loading {
  width: 100%;
  height: 100rpx;
}

.finish {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  color: var(--loading-text-color, #969799);
  font-size: 17px;
  text-align: center;
}